    "def INBS(s, ps):\n",
    "    A = []\n",
    "    index = s.find(ps)\n",
    "    while index != -1:\n",
    "        A.append((index, index + len(ps)))\n",
    "        index = s.find(ps, index + 1)\n",
    "    return A\n",
